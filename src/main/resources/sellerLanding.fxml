<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="bookstore.SellerController" alignment="CENTER" spacing="15" style="-fx-background-color: #222;">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Label fx:id="messageLabel" text="Welcome to Page 2!" style="-fx-text-fill: white; -fx-font-size: 16px;"/>
    <Button fx:id="randomButton" text="Click Me!" onAction="#handleRandomAction" style="-fx-background-color: #444; -fx-text-fill: white; -fx-background-radius: 5;"/>
</VBox>
